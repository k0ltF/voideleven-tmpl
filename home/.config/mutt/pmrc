# vim: filetype=muttrc

set mbox_type	= Maildir
set from 	= "username@pm.me"
set sendmail 	= "/usr/bin/msmtp -a ProtonMail"
set mbox	= +ProtonMail/INBOX
set record	= +ProtonMail/Sent
set postponed	= +ProtonMail/Drafts
set trash 	= +ProtonMail/Trash

color status green default

macro index ,e "<change-folder>+Gmail/INBOX<return>" "change to Gmail email"
